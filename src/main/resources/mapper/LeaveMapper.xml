<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzhb.zzoa.mapper.LeaveMapper">
	<insert id="addLeave" parameterType="com.zzhb.zzoa.domain.activiti.Leave">
		INSERT INTO oa_t_leave 
			(bk,sqr,bmmc,sqrq,qjlx,ksrq,jsrq,qjts,qjly,gzsc)
		VALUES
			(#{bk},#{sqr},#{bmmc},#{sqrq},#{qjlx},#{ksrq},#{jsrq},#{qjts},#{qjly},#{gzsc})
	</insert>
</mapper>