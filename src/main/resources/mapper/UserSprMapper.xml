<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzhb.zzoa.mapper.UserSprMapper">
	
	<select id="getUserSprs" resultType="com.zzhb.zzoa.domain.activiti.UserSpr" parameterType="java.util.Map">
		SELECT * FROM oa_t_user_spr
		<where>
			<if test="uid !=null and uid !='' ">
				and uid = #{uid}
			</if>
			<if test="key !=null and key !='' ">
				and key = #{key}
			</if>
		</where>
	</select>
	
</mapper>