  <process id="dispatchFile" name="发文稿纸" isExecutable="true" isClosed="false" processType="None">
    <extensionElements>
      <activiti:executionListener event="start" class="com.zzhb.listener.activiti.ProcessExecutionListener"></activiti:executionListener>
      <activiti:executionListener event="end" class="com.zzhb.listener.activiti.ProcessExecutionListener"></activiti:executionListener>
    </extensionElements>
    <userTask id="usertask2" name="会签" activiti:assignee="${leader}" activiti:formKey="dispatchFile_usertask3.html">
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="${leaderList}" activiti:elementVariable="leader">
        <completionCondition>${nrOfCompletedInstances/nrOfInstances == 1}</completionCondition>
      </multiInstanceLoopCharacteristics>
    </userTask>
    <userTask id="usertask3" name="办公室" activiti:formKey="dispatchFile_usertask2.html">
      <extensionElements>
        <activiti:taskListener event="create" class="com.zzhb.listener.activiti.UserTaskCreateListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask4" name="办公室" activiti:assignee="${bgsPerson}" activiti:formKey="dispatchFile_usertask5.html">
      <extensionElements>
        <activiti:taskListener event="create" class="com.zzhb.listener.activiti.UserTaskCreateListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <startEvent id="startevent1" name="填写发文" activiti:formKey="dispatchFile_startevent1.html"></startEvent>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow14" sourceRef="usertask4" targetRef="endevent3"></sequenceFlow>
    <sequenceFlow id="flow23" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <userTask id="usertask1" name="分管领导审核" activiti:formKey="dispatchFile_usertask1.html">
      <extensionElements>
        <activiti:taskListener event="create" class="com.zzhb.listener.activiti.UserTaskCreateListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow26" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow27" name="会签" sourceRef="usertask3" targetRef="usertask2">
      <extensionElements>
        <activiti:executionListener event="take" class="com.zzhb.listener.activiti.HuiQianListener"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${qfType == 1}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask5" name="签发" activiti:formKey="dispatchFile_usertask4.html">
      <extensionElements>
        <activiti:taskListener event="create" class="com.zzhb.listener.activiti.UserTaskCreateListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow28" sourceRef="usertask2" targetRef="usertask4"></sequenceFlow>
    <sequenceFlow id="flow29" sourceRef="usertask5" targetRef="usertask4"></sequenceFlow>
    <sequenceFlow id="flow30" name="签发" sourceRef="usertask3" targetRef="usertask5">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${qfType == 2}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow31" name="同意" sourceRef="exclusivegateway1" targetRef="usertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${agree == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow32" name="不同意" sourceRef="exclusivegateway1" targetRef="endevent3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${agree == false}]]></conditionExpression>
    </sequenceFlow>
  </process>
